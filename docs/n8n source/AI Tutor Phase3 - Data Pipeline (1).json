      "parameters": {
        "jsCode": "const extractedData = $('Extract File Info').first().json;\nconst fileId = extractedData.file_id;\nconst chunks = extractedData.chunks;\n\nconst items = chunks.map((chunk) => {\n  return {\n    json: {\n      file_id: fileId,\n      chunk_index: chunk.chunk_index,\n      content: chunk.text,\n      metadata: {\n        file_id: fileId,\n        chunk_index: chunk.chunk_index,\n        num_tokens: chunk.num_tokens,\n        headings: chunk.headings\n      }\n    }\n  };\n});\n\nreturn items;"
      },
