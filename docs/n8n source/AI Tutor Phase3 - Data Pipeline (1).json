    {
      "parameters": {
        "jsCode": "const response = $input.first().json;\nconst doclingChunks = response.chunks;\nconst firstChunk = doclingChunks[0];\n\nconst fileName = firstChunk.filename || 'Unknown File';\nconst fileId = `${fileName}_${Date.now()}`;\nconst mimeType = firstChunk.metadata?.origin?.mimetype || 'unknown';\n\n// 총 토큰 수 계산\nconst totalTokens = doclingChunks.reduce((sum, chunk) => sum + (chunk.num_tokens || 0), 0);\n\nreturn {\n  file_id: fileId,\n  title: fileName,\n  mimetype: mimeType,\n  total_chunks: doclingChunks.length,\n  total_tokens: totalTokens,\n  created_at: new Date().toISOString(),\n  chunks: doclingChunks\n};"
      },
